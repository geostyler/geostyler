    "@types/react": "^17.0.3",
    "@types/react-color": "^3.0.4",
    "@types/react-dom": "^17.0.3",
    "@ungap/url-search-params": "^0.2.2",
    "blob": "^0.1.0",
    "chroma-js": "^2.1.0",
    "codemirror": "^5.59.2",
    "color": "^3.1.3",
