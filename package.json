    "tsconfig-paths-webpack-plugin": "^3.2.0",
    "tslint": "^6.1.0",
    "typescript": "^3.8.2",
    "url-loader": "^3.0.0",
    "webpack": "^4.41.6",
    "webpack-cli": "^3.3.11",
    "whatwg-fetch": "^3.0.0"
  },
  "peerDependencies": {
    "antd": "3.x",
    "ol": "6.x",
    "react": "16.x",
    "react-dom": "16.x"
  }
}
