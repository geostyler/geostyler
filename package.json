{
  "name": "geostyler",
  "version": "0.18.0",
  "description": "Framework for styling geodata",
  "main": "dist/index.js",
  "files": [
    "dist",
    "browser"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/terrestris/geostyler.git"
  },
  "keywords": [
    "geo",
    "styler",
    "geostyler"
  ],
  "author": "",
  "license": "BSD-2-Clause",
  "bugs": {
    "url": "https://github.com/terrestris/geostyler/issues"
  },
  "homepage": ".",
  "scripts": {
    "build": "npm run build:dist && npm run build:app && npm run build:browser && npm run build:styleguide",
    "build:app": "react-scripts-ts build",
    "build:dist": "tsc -p ./ && copyfiles \"./src/**/*.css\" dist --up 1",
    "build:styleguide": "styleguidist build",
    "build:browser": "webpack --config browser-build.config.js",
    "eject": "react-scripts-ts eject",
    "lint": "tslint --project tsconfig.json --config tslint.json && tsc --noEmit --project tsconfig.json",
    "lint:prod": "tslint --project tsconfig.json --config tslint.prod.json && tsc --noEmit --project tsconfig.json",
    "pretest": "npm run lint:prod",
    "prebuild": "rimraf dist/** && rimraf build/**",
    "prepublishOnly": "npm run build",
    "release": "np --no-yarn && git push https://github.com/terrestris/geostyler.git master --tags",
    "styleguide": "styleguidist server",
    "start": "react-scripts-ts start",
    "start:styleguide": "styleguidist server",
    "test": "CI=true react-scripts-ts test --env=jsdom --runInBand --transformIgnorePatterns \"<rootDir>/node_modules/(?!ol|antd|codemirror)\"",
    "test:watch": "react-scripts-ts test --env=jsdom --transformIgnorePatterns \"<rootDir>/node_modules/(?!ol|antd|codemirror)\""
  },
  "dependencies": {
    "antd": "3.10.7",
    "blob": "0.0.5",
    "codemirror": "5.41.0",
    "color": "3.1.0",
    "file-saver": "1.3.8",
    "geostyler-data": "0.5.2",
    "geostyler-geojson-parser": "0.4.5",
    "geostyler-openlayers-parser": "0.18.2",
    "geostyler-sld-parser": "0.17.2",
    "geostyler-style": "0.14.2",
    "geostyler-wfs-parser": "0.7.3",
    "lodash": "4.17.11",
    "moment": "2.22.2",
    "ol": "4.6.5",
    "react": "16.6.0",
    "react-codemirror2": "5.1.0",
    "react-color": "2.14.1",
    "react-dom": "16.6.0",
    "react-rnd": "9.0.3",
    "react-scripts-ts": "3.1.0",
    "url-search-params": "1.1.0"
  },
  "devDependencies": {
    "@types/codemirror": "0.0.66",
    "@types/color": "3.0.0",
    "@babel/core": "7.1.5",
    "@types/enzyme": "3.1.15",
    "@types/file-saver": "2.0.0",
    "@types/geojson": "7946.0.4",
    "@types/jest": "23.3.9",
    "@types/lodash": "4.14.118",
    "@types/node": "10.12.5",
    "@types/ol": "4.6.2",
    "@babel/polyfill": "7.0.0",
    "@babel/preset-env": "7.1.5",
    "@babel/preset-stage-0": "7.0.0",
    "@types/prop-types": "15.5.6",
    "@types/react": "16.4.18",
    "@types/react-color": "2.13.6",
    "@types/react-dom": "16.0.9",
    "awesome-typescript-loader": "4.0.1",
    "babel-loader": "8.0.4",
    "canvas-prebuilt": "1.6.11",
    "copyfiles": "2.1.0",
    "coveralls": "3.0.1",
    "enzyme": "3.7.0",
    "enzyme-adapter-react-16": "1.6.0",
    "extract-text-webpack-plugin": "3.0.2",
    "np": "3.0.4",
    "react-docgen-typescript": "1.12.0",
    "react-styleguidist": "7.3.11",
    "rimraf": "2.6.2",
    "ts-jest": "22.4.6",
    "typescript": "3.1.4",
    "uglifyjs-webpack-plugin": "1.1.2",
    "webpack": "3.12.0",
    "webpack-cli": "3.1.2"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/Component/**/*.{tsx,jsx}",
      "src/DataProvider/**/*.{tsx,jsx}",
      "src/Util/**/*.{tsx,jsx}",
      "src/app/**/*.{tsx,jsx}"
    ]
  }
}
