    "@types/geojson": "^7946.0.10",
    "@types/lodash": "^4.14.188",
    "@types/react": "^18.0.25",
    "@types/react-color": "^3.0.6",
    "@types/react-dom": "^18.0.8",
    "@ungap/url-search-params": "^0.2.2",
    "blob": "^0.1.0",
    "chroma-js": "^2.4.2",
    "color": "^4.2.3",
    "csstype": "^3.1.1",
