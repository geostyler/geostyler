{
  "name": "geostyler",
  "version": "3.0.0",
  "description": "Framework for styling geodata",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "browser"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/terrestris/geostyler.git"
  },
  "keywords": [
    "geo",
    "styler",
    "geostyler"
  ],
  "author": "",
  "license": "BSD-2-Clause",
  "bugs": {
    "url": "https://github.com/terrestris/geostyler/issues"
  },
  "homepage": "https://github.com/terrestris/geostyler",
  "scripts": {
    "build": "npm run build:dist && npm run build:browser && npm run build:styleguide",
    "build:dist": "tsc -p ./ && copyfiles \"./src/**/*.css\" dist --up 1",
    "build:styleguide": "styleguidist build",
    "build:browser": "webpack --config browser-build.config.js",
    "lint": "tslint --project tsconfig.json --config tslint.json && tsc --noEmit --project tsconfig.json",
    "lint:prod": "tslint --project tsconfig.json --config tslint.prod.json && tsc --noEmit --project tsconfig.json",
    "pretest": "npm run lint:prod",
    "prebuild": "rimraf dist/** && rimraf build/**",
    "prepublishOnly": "npm run build",
    "release": "np --no-yarn && git push https://github.com/terrestris/geostyler.git master --tags",
    "styleguide": "styleguidist server",
    "start:dev": "webpack --mode=development --watch --config dev-build.config.js",
    "test": "jest --coverage",
    "test:ci": "jest --ci --coverage",
    "test:watch": "jest --watchAll"
  },
  "dependencies": {
    "@ungap/url-search-params": "0.1.2",
    "@types/chroma-js": "1.4.1",
    "@types/codemirror": "0.0.71",
    "@types/color": "3.0.0",
    "@types/file-saver": "2.0.0",
    "@types/geojson": "7946.0.5",
    "@types/lodash": "4.14.120",
    "@types/react": "16.8.1",
    "@types/react-color": "2.14.0",
    "@types/react-dom": "16.0.11",
    "blob": "0.0.5",
    "chroma-js": "2.0.2",
    "codemirror": "5.43.0",
    "color": "3.1.0",
    "file-saver": "2.0.0",
    "geostyler-cql-parser": "0.1.1",
    "geostyler-data": "0.5.3",
    "geostyler-geojson-parser": "0.4.6",
    "geostyler-openlayers-parser": "1.0.0",
    "geostyler-sld-parser": "0.18.0",
    "geostyler-style": "0.14.3",
    "geostyler-wfs-parser": "0.7.3",
    "lodash": "4.17.11",
    "moment": "2.24.0",
    "react-codemirror2": "5.1.0",
    "react-color": "2.17.0",
    "react-rnd": "9.1.1"
  },
  "devDependencies": {
    "@babel/core": "7.2.2",
    "@babel/polyfill": "7.2.5",
    "@babel/preset-env": "7.3.1",
    "@types/enzyme": "3.1.16",
    "@types/jest": "24.0.0",
    "@types/node": "10.12.21",
    "@types/prop-types": "15.5.8",
    "antd": "3.13.1",
    "autoprefixer": "9.4.7",
    "babel-core": "7.0.0-bridge.0",
    "babel-jest": "23.6.0",
    "babel-loader": "8.0.5",
    "canvas-prebuilt": "1.6.11",
    "case-sensitive-paths-webpack-plugin": "2.2.0",
    "copyfiles": "2.1.0",
    "coveralls": "3.0.2",
    "enzyme": "3.8.0",
    "enzyme-adapter-react-16": "1.8.0",
    "file-loader": "3.0.1",
    "fork-ts-checker-webpack-plugin": "0.5.2",
    "html-webpack-plugin": "3.2.0",
    "jest": "23.6.0",
    "mini-css-extract-plugin": "0.5.0",
    "np": "4.0.2",
    "ol": "5.3.0",
    "optimize-css-assets-webpack-plugin": "5.0.1",
    "postcss-flexbugs-fixes": "4.1.0",
    "postcss-loader": "3.0.0",
    "react": "16.7.0",
    "react-docgen-typescript": "1.12.3",
    "react-dom": "16.7.0",
    "react-styleguidist": "8.0.6",
    "rimraf": "2.6.3",
    "source-map-loader": "0.2.4",
    "terser-webpack-plugin": "1.2.2",
    "ts-jest": "23.10.5",
    "ts-loader": "5.3.3",
    "tsconfig-paths-webpack-plugin": "3.2.0",
    "tslint": "5.12.1",
    "typescript": "3.3.1",
    "url-loader": "1.1.2",
    "webpack": "4.29.0",
    "webpack-cli": "3.2.1",
    "whatwg-fetch": "3.0.0"
  },
  "peerDependencies": {
    "antd": "3.x",
    "ol": "5.x",
    "react": "16.x",
    "react-dom": "16.x"
  }
}
