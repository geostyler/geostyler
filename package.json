    "copyfiles": "2.0.0",
    "coveralls": "3.0.1",
    "enzyme": "3.3.0",
    "enzyme-adapter-react-16": "1.1.1",
    "np": "3.0.4",
    "react-docgen-typescript": "1.6.2",
    "react-styleguidist": "7.1.3",
    "rimraf": "2.6.2",
    "ts-jest": "23.1.2",
    "typescript": "2.9.2"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/Component/**/*.{tsx,jsx}",
      "src/DataProvider/**/*.{tsx,jsx}",
      "src/Util/**/*.{tsx,jsx}",
      "src/app/**/*.{tsx,jsx}"
    ]
  }
