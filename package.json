{
  "name": "geostyler",
  "version": "8.1.0",
  "description": "Framework for styling geodata",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "browser"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/geostyler/geostyler.git"
  },
  "keywords": [
    "geo",
    "styler",
    "geostyler"
  ],
  "author": "",
  "license": "BSD-2-Clause",
  "bugs": {
    "url": "https://github.com/geostyler/geostyler/issues"
  },
  "engines": {
    "node": ">=10.0.0"
  },
  "browserslist": [
    ">1%",
    "last 4 versions",
    "Firefox ESR",
    "not ie < 9"
  ],
  "homepage": "https://github.com/geostyler/geostyler",
  "scripts": {
    "build": "npm run build-dist && npm run build-browser && npm run build-styleguide",
    "build-dist": "tsc -p ./ && copyfiles \"./src/**/*.less\" dist --up 1 && node transform-less.js",
    "build-styleguide": "styleguidist build",
    "build-browser": "webpack --config webpack.browser.config.js",
    "lint": "eslint -c .eslintrc.js --ext .ts,tsx src/ && tsc --noEmit --project tsconfig.json",
    "lint:test": "npm run lint && npm run test",
    "lint:test:cleanup:build": "npm run lint && npm run test && npm run cleanup && npm run build",
    "cleanup": "rimraf dist/** && rimraf build/**",
    "prepublishOnly": "npm run build-dist && npm run build-browser",
    "release": "np --no-yarn && git push https://github.com/geostyler/geostyler.git master",
    "styleguide": "styleguidist server",
    "start-dev": "webpack --watch --config webpack.dev.config.js",
    "test": "jest --coverage",
    "test-ci": "jest --ci --coverage",
    "test-watch": "jest --watchAll"
  },
  "dependencies": {
    "@babel/polyfill": "^7.12.1",
    "@monaco-editor/react": "^4.3.1",
    "@types/chroma-js": "^2.1.3",
    "@types/color": "^3.0.3",
    "@types/file-saver": "^2.0.5",
    "@types/geojson": "^7946.0.8",
    "@types/lodash": "^4.14.180",
    "@types/react": "^17.0.40",
    "@types/react-color": "^3.0.6",
    "@types/react-dom": "^17.0.13",
    "@ungap/url-search-params": "^0.2.2",
    "blob": "^0.1.0",
    "chroma-js": "^2.4.2",
    "color": "^4.2.1",
    "csstype": "^3.0.11",
    "file-saver": "^2.0.5",
    "geostyler-cql-parser": "^1.1.0",
    "geostyler-data": "^1.0.0",
    "geostyler-geojson-parser": "^1.0.1",
    "geostyler-openlayers-parser": "^3.0.2",
    "geostyler-sld-parser": "^3.2.2",
    "geostyler-style": "^5.0.2",
    "geostyler-wfs-parser": "^1.0.1",
    "lodash": "^4.17.21",
    "moment": "^2.29.1",
    "monaco-editor": "^0.33.0",
    "react-color": "^2.19.3",
    "react-rnd": "^10.3.5",
    "typescript-json-schema": "^0.53.0"
  },
  "devDependencies": {
    "@ant-design/icons": "^4.7.0",
    "@babel/core": "^7.17.7",
    "@babel/plugin-proposal-class-properties": "^7.16.7",
    "@babel/plugin-proposal-object-rest-spread": "^7.17.3",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-react": "^7.16.7",
    "@babel/preset-typescript": "^7.16.7",
    "@babel/runtime": "^7.17.7",
    "@terrestris/eslint-config-typescript-react": "^2.0.0",
    "@testing-library/dom": "^8.11.3",
    "@testing-library/jest-dom": "^5.16.2",
    "@testing-library/react": "^12.1.4",
    "@testing-library/user-event": "^13.5.0",
    "@types/enzyme": "^3.10.11",
    "@types/jest": "^27.4.1",
    "@types/jest-diff": "^24.3.0",
    "@types/node": "^16.11.26",
    "@types/webpack": "^5.28.0",
    "@typescript-eslint/eslint-plugin": "^5.15.0",
    "@typescript-eslint/eslint-plugin-tslint": "^5.15.0",
    "@typescript-eslint/parser": "^5.15.0",
    "antd": "^4.16.3",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^27.5.1",
    "babel-loader": "^8.2.3",
    "buffer": "^6.0.3",
    "copyfiles": "^2.4.1",
    "coveralls": "^3.1.1",
    "css-loader": "^6.7.1",
    "css-minimizer-webpack-plugin": "^3.4.1",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.6",
    "eslint": "^8.11.0",
    "eslint-plugin-react": "^7.29.4",
    "eslint-plugin-react-hooks": "^4.3.0",
    "fork-ts-checker-webpack-plugin": "^7.2.1",
    "jest": "^27.5.1",
    "less": "^4.1.2",
    "less-loader": "^10.2.0",
    "mini-css-extract-plugin": "^2.6.0",
    "np": "^7.6.0",
    "ol": "^6.13.0",
