    "react-dom": "^17.0.2",
    "react-styleguidist": "^11.2.0",
    "readdirp": "^3.6.0",
    "replace-in-file": "^6.3.2",
    "rimraf": "^3.0.2",
    "terser-webpack-plugin": "^5.3.1",
    "ts-loader": "^9.2.8",
    "typescript": "^4.6.2",
    "webpack": "^5.70.0",
    "webpack-cli": "^4.9.2",
    "whatwg-fetch": "^3.6.2"
  },
  "peerDependencies": {
    "@ant-design/icons": "4.x",
    "antd": "4.x",
    "ol": "6.x",
    "react": ">=16.x",
    "react-dom": ">=16.x"
  }
}
